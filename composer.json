{
  "name": "messageway/messagewayphp",
  "description": "This SDK allows you to connect to MSGWay.com server from your php application.",
  "type": "library",
  "keywords": [
    "message",
    "otp",
    "sms",
    "ivr"
  ],
  "license": "MIT",
  "homepage": "https://MSGway.com",
  "authors": [
    {
      "name": "Ehsan Sabet",
      "email": "ehsan.sabet@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "MessageWay\\Api\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "9.5.x-dev"
  }
}
